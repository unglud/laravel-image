{
  "name": "unglud/laravel-image",
  "description": "Basic image saver for Laravel 5",
  "keywords": [
    "laravel",
    "image",
    "upload",
    "file",
    "save"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Aleksandr Matrosov",
      "email": "unglud@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Unglued\\LavaImage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.1",
    "intervention/image": "~2",
    "illuminate/support": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^0.9.4"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Unglued\\LavaImage\\LavaImageServiceProvider"
      ]
    },
    "aliases": {
      "LavaImage": "Unglued\\LavaImage\\Facades\\LavaImage"
    }
  }
}
